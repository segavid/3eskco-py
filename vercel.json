{
  "version": 2,
  "builds": [
    { "src": "api/index.py", "use": "@vercel/python" }
  ],
  "routes": [
    {
      "src": "/(.*\\.(css|js|jpg|jpeg|png|gif|svg|ico|woff|woff2|ttf|eot|webp|mp4|webm))",
      "dest": "https://v.3esk.co/watch/$1",
      "headers": { "Cache-Control": "public, max-age=31536000" }
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ]
}
